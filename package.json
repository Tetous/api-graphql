{
  "name": "api-graphql",
  "version": "1.0.0",
  "main": "dist/main.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "cross-env NODE_ENV=development tsnd -r tsconfig-paths/register --transpile-only --clear --ignore-watch {node_modules,__tests__} src/main.ts",
    "start": "cross-env NODE_ENV=production node dist/main.js",
    "prebuild": "yarn clean",
    "build": "ttsc",
    "typeorm": "tsnd -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles",
    "test:watch": "yarn test --watchAll",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "dependencies": {
    "apollo-server": "^2.19.2",
    "apollo-server-testing": "^2.19.2",
    "async": "^3.2.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.0",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "date-fns": "^2.16.1",
    "graphql": "^15.4.0",
    "handlebars": "^4.7.6",
    "ioredis": "^4.19.4",
    "jsonwebtoken": "^8.5.1",
    "jw-paginate": "^1.0.4",
    "merge-graphql-schemas": "^1.7.8",
    "nodemailer": "^6.4.17",
    "pg": "^8.5.1",
    "rate-limiter-flexible": "^2.2.1",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.4.0",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.30"
  },
  "devDependencies": {
    "@types/async": "^3.2.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/ioredis": "^4.17.11",
    "@types/jest": "26.0.20",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "14.14.21",
    "@types/nodemailer": "^6.4.0",
    "@types/redis": "^2.8.28",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "@zerollup/ts-transform-paths": "1.7.18",
    "cross-env": "7.0.3",
    "dotenv": "8.2.0",
    "eslint": "7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.4.4",
    "ts-node-dev": "1.1.1",
    "tsconfig-paths": "3.9.0",
    "ttypescript": "1.5.12",
    "typescript": "4.1.3"
  }
}
